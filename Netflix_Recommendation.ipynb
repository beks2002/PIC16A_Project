   "source": [
    "For ease of use, let's get rid of all the spaces, ',' and change uppercase letters to lowercase letters."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "741c39d1",
   "metadata": {},
   "outputs": [],
   "source": [
    "def separate(texts):\n",
    "    t = []\n",
    "    for text in texts.split(','):\n",
    "        t.append(text.replace(' ', '').lower())\n",
    "    \n",
    "    return ' '.join(t)\n",
    "\n",
    "def remove_space(texts):\n",
    "    return texts.replace(' ', '').lower()\n",
    "\n",
    "def remove_punc(texts):\n",
    "    return texts.translate(str.maketrans('', '', string.punctuation)).lower()"
   ]
  },
